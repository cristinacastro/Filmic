onent } from \"react\";\nimport Service from \"../lib/Service\";\nimport NavBar from \"../components/Navbar\";\nimport { Link } from \"react-router-dom\";\n\nclass Admin extends Component {\n  state = {\n    movies: [],\n    randomMovie: [],\n  };\n\n  componentDidMount() {\n    this.getAllMovies();\n    this.getRandomMovie();\n  }\n\n  getAllMovies () {\n    Service.getAllMovies().then((data) => {\n      this.setState({\n        movies: data,\n      });\n    })\n  }\n\n  getRandomMovie () {\n    Service.getRandomUnratedMovie().then((data) => {\n      this.setState({\n        randomMovie: data,\n      });\n    })\n  }\n\n  render() {\n    return (\n      <div>\n        <NavBar />\n        <section>\n          <h2>{this.state.randomMovie.title}</h2>\n          <Link to={`/movies/${this.state.randomMovie._id}`}>Discover</Link> \n          <br></br><img src={this.state.randomMovie.poster_url} />\n        </section>\n\n        <section>\n          <form>\n            Buscador\n          </form>\n\n        </section>\n        <section>\n        <h2>Latest releases</h2>\n        {this.state.movies.map((eachMovie) => {\n            return (\n              <div key={eachMovie.id}>\n              <Link to={{pathname:`/movies/${eachMovie.id}`, state: {moviesState:this.state.movies}}}>\n                <img src={eachMovie.poster_url}/>\n                <h3>{eachMovie.title}</h3>\n                <p>See movie</p>\n                </Link>\n\n              </div>\n            )\n          })}\n        </section>\n      </div>\n    );\n  }\n}\nexport default Admin;\n","/home/cristina/Documents/Projects/filmic/filmic/src/pages/Home.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/context/auth.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/components/PrivateRoute.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/pages/Login.js",["72","73"],"import React, { useState } from \"react\";\nimport { Link, Redirect } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { Card, Form, Input, Button, Error } from \"../components/AuthForms\";\nimport { useAuth } from \"../context/auth\";\n\nfunction Login(props) {\n  const [isLoggedIn, setLoggedIn] = useState(false);\n  const [isError, setIsError] = useState(false);\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const { setAuthTokens } = useAuth();\n  const apiurl =\n    \"https://front-assignment.planetdataset.com/api/v1/candidates/42b619d4-e3b0-445c-89de-e0cf4261ee5a/users/login\";\n\n  const referer = props.location.state.referer || \"/\";\n\n  function postLogin() {\n    axios\n      .post(apiurl, {\n        email,\n        password,\n      })\n      .then((result) => {\n        if (result.status === 200) {\n          setAuthTokens(result.data);\n          setLoggedIn(true);\n        } else {\n          setIsError(true);\n        }\n      })\n      .catch((e) => {\n        setIsError(true);\n      });\n  }\n\n  if (isLoggedIn) {\n    return <Redirect to={referer} />;\n  }\n\n  return (\n    <Card>\n      <Form>\n        <Input\n          type=\"email\"\n          value={email}\n          onChange={(e) => {\n            setEmail(e.target.value);\n          }}\n          placeholder=\"email\"\n        />\n        <Input\n          type=\"password\"\n          value={password}\n          onChange={(e) => {\n            setPassword(e.target.value);\n          }}\n          placeholder=\"password\"\n        />\n        <Button onClick={postLogin}>Sign In</Button>\n      </Form>\n      <Link to=\"/signup\">Don't have an account?</Link>\n      {/* {isError && <Error>The email or password provided were incorrect!</Error>} */}\n    </Card>\n  );\n}\n\nexport default Login;\n","/home/cristina/Documents/Projects/filmic/filmic/src/pages/Signup.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/components/AuthForms.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/lib/Service.js",[],"/home/cristina/