 => {\n        setIsError(true);\n      });\n  }\n\n  if (isLoggedIn) {\n    return <Redirect to={referer} />;\n  }\n\n  return (\n    <Card>\n      <Form>\n        <Input\n          type=\"email\"\n          value={email}\n          onChange={(e) => {\n            setEmail(e.target.value);\n          }}\n          placeholder=\"email\"\n        />\n        <Input\n          type=\"password\"\n          value={password}\n          onChange={(e) => {\n            setPassword(e.target.value);\n          }}\n          placeholder=\"password\"\n        />\n        <Button onClick={postLogin}>Sign In</Button>\n      </Form>\n      <Link to=\"/signup\">Don't have an account?</Link>\n      {/* {isError && <Error>The email or password provided were incorrect!</Error>} */}\n    </Card>\n  );\n}\n\nexport default Login;\n","/home/cristina/Documents/Projects/filmic/filmic/src/pages/Signup.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/components/AuthForms.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/lib/Service.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/components/Navbar.js",[],"/home/cristina/Documents/Projects/filmic/filmic/src/pages/Movies.js",["83"],"import React, { Component } from \"react\";\nimport Service from \"../lib/Service\";\nimport NavBar from \"../components/Navbar\";\nimport { Link } from \"react-router-dom\";\n\nclass Movies extends Component {\n    \n  constructor(props){\n    super(props)\n    this.state = {\n      movies: [],\n    };\n  } \n\n\n  componentDidMount() {\n    this.getAllMovies();\n  }\n\n  getAllMovies () {\n    Service.getAllMovies().then((data) => {\n      this.setState({\n        movies: data,\n      });\n      console.log(this.state.movies, \"the movies\")\n    })\n  }\n\n\n  render() {\n    return (\n      <div>\n        <NavBar />\n        <section>\n        <h2>Movies</h2>\n          {this.state.movies.map((eachMovie) => {\n            return (\n              <div key={eachMovie.id}>\n       